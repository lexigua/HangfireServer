<!DOCTYPE html>
<html>
<head>
   
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <link rel="icon" href="favicon.ico" type="image/x-icon"/>
    <link rel="SHORTCUT ICON" href="favicon.ico" type="image/x-icon"/>
    <title>@ViewBag.Title</title>
    <link type="text/css" rel="stylesheet" href="/Scripts/EasyUi/themes/default/easyui.css" id="skin"/>
    @Styles.Render("~/Css/css")
    <link href="~/Css/default.css" rel="stylesheet"/>
    <link type="text/css" rel="stylesheet" href="/Css/Icons-upload/icon.css"/>
    <link type="text/css" rel="stylesheet" href="/Scripts/WebUploader/webuploader.css"/>
    <script type="text/javascript" src="/Scripts/JQuery/jquery.min.js"></script>
    <script type="text/javascript" src="/Scripts/JQuery/jquery.json-2.4.min.js"></script>
    <script type="text/javascript" src="/Scripts/WebUploader/webuploader.min.js"></script>
    <script type="text/javascript" src="/Scripts/EasyUi/jquery.easyui.min.js"></script>
    <script src="~/Scripts/Projects/CommonMethod.js"></script>
    <script src="~/Scripts/EChart/echarts.common.min.js"></script>
    <script src="~/Scripts/EChart/echarts.min.js"></script>
    <script src="~/Scripts/Common/Export.js"></script>
    <script src="~/Scripts/ProjectJson.js"></script>
    <script src="~/Scripts/date_noScroll.js"></script>
    @Scripts.Render("~/Scripts/EasyUi/js")
    @Scripts.Render("~/Scripts/Utils/js")

    @RenderSection("head", false)
    @RenderSection("content", false)
    <meta name="format-detection" content="telephone=no" />
    <!--[if lt IE 9]>
        <script src="~/Scripts/html5.js"></script>
    <![endif]-->

    <script type="text/javascript">
        $(function() {
            $(".easyui-linkbutton").bind('keydown', function(e) {
                if (e.keyCode == 13) {
                    return false;
                }
            });
        });

    </script>
</head>
<body id="divMain" class="easyui-layout">
    @RenderBody()
    <div id="masklayerDiv"></div>
</body>
</html> 